    "expo-asset": "8.4.6",
    "expo-constants": "13.0.1",
    "expo-font": "10.0.4",
    "expo-updates": "0.11.6",
    "expo-web-browser": "10.1.0",
    "polished": "4.2.2",
    "prettier": "2.6.2",
    "react": "17.0.1",
    "react-native": "0.64.3",
