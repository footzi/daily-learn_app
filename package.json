{
  "name": "dailylearn",
  "author": "Vladislav Manakov <footzi@mail.ru>",
  "description": "Application for learning english words",
  "repository": "https://github.com/footzi/daily-learn_app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:cache": "expo start -c",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo web --port=3000",
    "eject": "expo eject",
    "lint": "eslint . --ext .ts,.tsx && npm run tsc",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test": "jest --watchAll",
    "build-android-sdk": "expo build:android -t apk",
    "prettier": "prettier --write **/*.ts **/*.tsx",
    "expo-upgrade": "expo upgrade",
    "tsc": "tsc",
    "t": "yarn cache clean && yarn && expo start --clear"
  },
  "jest": {
    "preset": "jest-expo",
    "moduleNameMapper": {
      "^@constants": "<rootDir>/constants",
      "^@api": "<rootDir>/api/",
      "^@libs": "<rootDir>/libs/",
      "^@store": "<rootDir>/store/",
      "^@mocks": "<rootDir>/mocks/",
      "^@components": "<rootDir>/components/",
      "^@assets": "<rootDir>/assets/"
    }
  },
  "dependencies": {
    "@babel/plugin-transform-modules-commonjs": "7.17.9",
    "@expo/samples": "3.0.3",
    "@expo/vector-icons": "12.0.5",
