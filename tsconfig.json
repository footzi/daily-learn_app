{
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "noFallthroughCasesInSwitch": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "isolatedModules": true,
//    "strict": true,
    "jsx": "react-native",
    "lib": ["es6"],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "target": "esnext",
    "paths": {
      "@constants": [
        "constants"
      ],
      "@api": [
        "api"
      ],
      "@libs": [
        "libs"
      ],
      "@store": [
        "store"
      ],
      "@mocks": [
        "mocks"
      ],
      "@components": [
        "components"
      ],
      "@assets": [
        "assets"
      ],
      "@interfaces": [
        "interfaces"
      ],
      "@navigation/*": [
        "navigation/*"
      ]
    }
  },
  "exclude": ["node_modules"],
  "extends": "expo/tsconfig.base"
}
